{
  "name": "laravel-profiler-client",
  "version": "1.3.0",
  "description": "Profiler Client and Profiler Server for Laravel 5.2+",
  "keywords": [
    "laravel",
    "profiler",
    "debugbar"
  ],
  "homepage": "https://github.com/jkocik/laravel-profiler",
  "license": "MIT",
  "author": "Janusz Kocik <janusz.kocik@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jkocik/laravel-profiler-client.git"
  },
  "files": [
    "/dist",
    "/server",
    "LICENSE.md",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "test": "cross-env BABEL_ENV=test karma start test/feature/karma.conf.js",
    "e2e": "cross-env BABEL_ENV=test node test/e2e/runner.js",
    "lint": "eslint --ext .js,.vue src test/feature test/e2e/specs",
    "build": "node build/build.js",
    "coveralls": "cat test/feature/coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "profiler-server": "node server/server.js http=8099 ws=1901",
    "profiler-client": "http-server dist/",
    "ps": "npm run profiler-server",
    "pc": "npm run profiler-client"
  },
  "dependencies": {
    "express": "^4.16.3",
    "http-server": "^0.11.1",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
